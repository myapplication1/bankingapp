@model app.com.Data.app_branch
@{
    ViewBag.Title = "Branch";
    ViewBag.Description = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (ViewBag.alert == "error")
{
    <div class="alert alert-danger fade in">
        <button class="close" data-dismiss="alert">
            ×
        </button>
        <i class="fa-fw fa fa-times"></i>
        <strong>Info !</strong> @Html.ValidationSummary(false)


    </div>

}
@if (ViewBag.alert == "saved")
{
    <div class="alert alert-danger fade in">
        <button class="close" data-dismiss="alert">
            ×
        </button>
        <i class="fa-fw fa fa-times"></i>
        <strong>Info !</strong> Data Saved.
    </div>


}
<form id="html5Form" method="post" class="form-horizontal" action="@Url.Action("Edit", "bra")">
    @Html.AntiForgeryToken()
    @*-----save andcancel button *@
    <div class="form-group">
        <div class="col-lg-offset-4 col-lg-8">
            <input name="btn_edit" id="btn_edit" class="btn btn-palegreen" type="submit" value="Save" />
            <input name="btn_cancel" id="btn_cancel" class="btn btn btn-danger" value="Cancel" onclick="@("window.location.href='" + @Url.Action("Index", "bra") + "'");" />


        </div>
    </div>

    <div class="col-lg-12 col-sm-6 col-xs-12">
        <div class="widget flat radius-bordered">
